        <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
            <ActivityIndicator
                HeightRequest="150"
                IsRunning="True"
                WidthRequest="150" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>